

MyBaseClass = class
  word = 'hello'
  say = function()
    word
  end
end

MyClass = class extends MyBaseClass

  initialize = function(value)
    word = value
  end

  say = function()
    temp = ''
    value = null
    if true
      temp = base.say() + "yes"
    else
      temp = base.say() + "no"
    end
    temp
  end

end

test = MyClass.new('hihi')

print(test.say())

