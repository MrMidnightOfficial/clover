

MyBaseClass = class
  word = 'hello'
  say = function()
    word
  end
end

MyClass = class extends MyBaseClass

  initialize = function(value)
    word = value
  end

  say = function()
    temp = ''
    value = null
    if true
      temp = base.say() + "yes"
    else
      temp = base.say() + "no"
    end
    temp
  end

  op_plus = function(target)
    'hahahahahaha'
  end

  op_uminus = function()
    'ohohohohoh'
  end
end


OuterClass = class
  my_class = MyClass.new('test')
end

outer = OuterClass.new()

print(outer.my_class.word)
print(outer['my_class']['say']())

print(outer.my_class + 1)

